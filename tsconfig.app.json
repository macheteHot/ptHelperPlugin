{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "rollup.config.js"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "Node",
    "types": ["chrome", "element-plus/global"],
    "lib": ["DOM", "DOM.Iterable", "ES6"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
